<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd" caption="msg://CaptionCostCopy"
        class="com.haulmont.thesis.crm.web.cost.copy.CostCopyEditor" messagesPack="com.haulmont.thesis.crm.web.cost.copy">
    <layout>
        <vbox spacing="true">
            <vbox spacing="true">
                <pickerField id="project" metaClass="crm$Project" caption="msg://msgProject" required="true" width="100%"/>
                <dateField id="startDate" dateFormat="dd.MM.yyyy" caption="msg://startDate" width="100%"/>
            </vbox>
            <buttonsPanel id="buttonsPanel"
                          align="MIDDLE_LEFT"
                          width="100%">
                <button id="selectButton"
                        caption="Далее"
                        width="100%"/>
                <button id="closeButton"
                        caption="Закрыть"
                        width="100%"/>
            </buttonsPanel>
        </vbox>
    </layout>
</window>
